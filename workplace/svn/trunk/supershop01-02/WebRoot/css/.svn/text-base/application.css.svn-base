body,tr,td,th,div,span,ul,li,select,option,input{
    font-size:12px;
    font-family:verdana;
    padding:0px;
    margin:0px;   
}

#header{
   height:80px;	
   border-bottom:2px solid gray;
}

#productName{
   font-size:30px;
   font-weight:bold;
   float:left;
   padding:10px;
   margin-top:20px;
}

#navigator{
   width:130px;
   padding:10px;
   height:480px;
   border-right:2px solid gray;
   float:left;	
}

#navigator .menuitem{
   font-size:14px;
   text-align:center;
   margin-top:8px;
}

#navigator .seperator{
   border-bottom:1px dotted gray;
   margin-top:10px;
   margin-bottom:10px;
}
#content{
   border-top:1px solid gray;
   border-left:1px solid gray;
   max-height: 480px;
   float:left;
}
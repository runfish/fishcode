public class Order extends ObjectValue {

	/**
	 * 订单编号
	 */
	private Integer orderId; 
	/**
	 * 下单时间
	 */
	private String orderTime; 
	/**
	 * 订单状态
	 */
	private String orderStatus; 
	/**
	 * 订单金额
	 */
	private Double orderSum;  
	/**
	 * 订单商品总数
	 */
	private Integer orderGoodsQty;
	/**
	 * 订单商品编号
	 */
	private Integer orderGoodsId; 
	/**
	 * 订单用户编号
	 */
	private Integer orderCustomerId;
	/**
	 * 订单管理员编号
	 */
	private Integer orderAdminId;
   /**
    * 获取订单的编号
    * @return Integer
    */
	public Integer getOrderId() {
		return orderId;
	}
	/**
	 * 修改订单编号
	 * @param orderId --订单编号
	 */
	public void setOrderId(Integer orderId) {
		this.orderId = orderId;
	}
	/**
	 * 获取下单时间
	 * @return String
	 */
	public String getOrderTime() {
		return orderTime;
	}
	/**
	 * 修改下单时间
	 * @param orderTime  --下单时间
	 */
	public void setOrderTime(String orderTime) {
		this.orderTime = orderTime;
	}
	/**
	 * 获取订单当前状态
	 * @return String
	 */
	public String getOrderStatus() {
		return orderStatus;
	}
	/**
	 * 修改当前的订单状态
	 * @param orderStatus -订单状态
	 */
	public void setOrderStatus(String orderStatus) {
		this.orderStatus = orderStatus;
	}
	/**
	 * 获取订单金额
	 * @return Double
	 */
	public Double getOrderSum() {
		return orderSum;
	}
	/**
	 * 修改订单金额
	 * @param orderSum -订单金额
	 */
	public void setOrderSum(Double orderSum) {
		this.orderSum = orderSum;
	}
	/**
	 * 获取订单的商品数量
	 * @return Integer
	 */
	public Integer getOrderGoodsQty() {
		return orderGoodsQty;
	}
	/**
	 * 修改订单的商品数量
	 * @param orderGoodsQty -订单商品数量
	 */
	public void setOrderGoodsQty(Integer orderGoodsQty) {
		this.orderGoodsQty = orderGoodsQty;
	}
	/**
	 * 获取订单的商品所属的商品的商品编号
	 * @return Integer
	 */
	public Integer getOrderGoodsId() {
		return orderGoodsId;
	}
	/**
	 * 修改订单的商品所属的商品的商品编号
	 * @param orderGoodsId -订单所属商品编号
	 */
	public void setOrderGoodsId(Integer orderGoodsId) {
		this.orderGoodsId = orderGoodsId;
	}
	/**
	 * 获取下单的用户编号
	 * @return Integer
	 */
	public Integer getOrderCustomerId() {
		return orderCustomerId;
	}
	/**
	 * 修改下单的用户编号
	 * @param orderCustomerId -订单所属用户编号
	 */
	public void setOrderCustomerId(Integer orderCustomerId) {
		this.orderCustomerId = orderCustomerId;
	}
	/**
	 * 获取管理该类商品订单的管理员编号
	 * @return Integer
	 */
	public Integer getOrderAdminId() {
		return orderAdminId;
	}
	/**
	 * 修改管理该类商品的管理员编号
	 * @param orderAdminId  -订单所属的管理员编号
	 */
	public void setOrderAdminId(Integer orderAdminId) {
		this.orderAdminId = orderAdminId;
	}
}

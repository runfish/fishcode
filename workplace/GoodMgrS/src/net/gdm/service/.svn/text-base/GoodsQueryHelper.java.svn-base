package net.gdm.service;

public class GoodsQueryHelper {
	private Integer bCtg;
	private Integer sCtg;
	private String  goodsName;
	
	
	public Integer getBCtg() {
		return bCtg;
	}
	public void setBCtg(Integer ctg) {
		bCtg = ctg;
	}
	public Integer getSCtg() {
		return sCtg;
	}
	public void setSCtg(Integer ctg) {
		sCtg = ctg;
	}
	public String getGoodsName() {
		return goodsName;
	}
	public void setGoodsName(String goodsName) {
		this.goodsName = goodsName;
	}
	

}
